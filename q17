






#include <stdlib.h>
#include<iostream>
#include<time.h>
using  namespace std;

void soma(int n, int *a, int *b,int *c){

for(int i= 0; i<n;i++){
  c[i]=a[i]+b[i];
  cout<<a[i]+b[i]<<endl;
}
}
int main(){
int n;
cout<<"digite tamanha do vetor "<<endl;
cin>>n;
int a[n],b[n];

cout<<"v1"<<endl;
for(int i= 0; i<n;i++){
  cin>>a[i];
}

cout<<"v2"<<endl;
for(int i= 0; i<n;i++){
  cin>>b[i];
}
int s[n];
soma(n,a,b,s);

   return 0 ;
}

