#include<stdlib.h>
#include<iostream>
using namespace std;
int main(){

  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
  printf("%d ",*(vet+i));
  }


  return 0 ;
} 

resposta: imprime 4 9 13 


